{
  "name": "@trafficguard/nous",
  "version": "0.0.1-dev",
  "description": "Autonomous AI agent framework and AI software developer",
  "private": true,
  "engines": {
    "node": ">=20.6.0"
  },
  "scripts": {
    "ai": "node --env-file=.env -r ts-node/register -r tsconfig-paths/register src/test/chat.ts",
    "agent": "node --env-file=.env -r ts-node/register -r tsconfig-paths/register src/test/agent.ts",
    "edit-local": "node --env-file=.env -r ts-node/register -r tsconfig-paths/register src/test/edit-local.ts",
    "research": "node --env-file=.env -r ts-node/register -r tsconfig-paths/register src/test/research.ts",
    "util": "node --env-file=.env -r ts-node/register -r tsconfig-paths/register --inspect=0.0.0.0:9229 src/test/util.ts",
    "build": "tsc",
    "build:dev": "tsc -project tsconfig.swc.json",
    "run": "node --env-file=.env -r ts-node/register -r tsconfig-paths/register src/index.ts",
    "run:dev": "ts-node -r tsconfig-paths/register --project tsconfig.swc.json src/index.ts",
    "watch": "./node_modules/tsc-watch/index.js --onSuccess \"node --env-file=variables/Development dist/index.js\"",
    "test": "npm run test:unit && echo \"No system or integration tests\"",
    "test:unit": "node --env-file=.env ./node_modules/mocha/bin/mocha -r ts-node/register -r tsconfig-paths/register \"src/**/*.test.[jt]s\"",
    "test:unit:dev": "TS_NODE_PROJECT='./tsconfig.swc.json' node  --env-file=.env ./node_modules/mocha/bin/mocha -r ts-node/register -r tsconfig-paths/register -t 0 --exit --colors \"src/**/*.test.[jt]s\"",
    "test:integration": "node --env-file=variables/Development ./node_modules/mocha/bin/mocha -r ts-node/register -r tsconfig-paths/register \"tests/integration/**/*.spec.ts\" --timeout 0  --exit",
    "test:system": "node --env-file=variables/Development ./node_modules/mocha/bin/mocha -r ts-node/register -r tsconfig-paths/register \"src/**/*.sys.ts\" --timeout 0  --exit",
    "test:single": "node --env-file=variables/Development ./node_modules/mocha/bin/mocha -r ts-node/register -r tsconfig-paths/register $1 --timeout 0 --colors --exit --",
    "format": "biome format --write ./src",
    "lint": "biome check --apply-unsafe ./src"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.16.1",
    "@anthropic-ai/vertex-sdk": "^0.2.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/type-provider-typebox": "^4.0.0",
    "@gitbeaker/rest": "^39.34.3",
    "@google-cloud/opentelemetry-cloud-trace-exporter": "^2.1.0",
    "@google-cloud/storage": "^7.9.0",
    "@google-cloud/vertexai": "^1.0.0",
    "@grpc/grpc-js": "^1.8.0",
    "@mozilla/readability": "^0.5.0",
    "@opentelemetry/api": "^1.6.0",
    "@opentelemetry/auto-instrumentations-node": "^0.44.0",
    "@opentelemetry/instrumentation": "^0.50.0",
    "@opentelemetry/instrumentation-fastify": "^0.35.0",
    "@opentelemetry/instrumentation-http": "^0.50.0",
    "@opentelemetry/resources": "^1.23.0",
    "@opentelemetry/sdk-node": "^0.50.0",
    "@opentelemetry/sdk-trace-base": "^1.23.0",
    "@opentelemetry/semantic-conventions": "^1.17.1",
    "@sinclair/typebox": "^0.32.20",
    "@types/pg": "^8.11.4",
    "api": "^6.1.1",
    "axios": "^1.6.7",
    "axios-retry": "^4.1.0",
    "crawlee": "^3.8.1",
    "drizzle-orm": "^0.30.8",
    "dropbox": "^10.34.0",
    "fast-xml-parser": "^4.3.6",
    "fastify": "^4.26.2",
    "fastify-healthcheck": "^4.4.0",
    "fastify-raw-body": "^4.3.0",
    "googleapis": "^126.0.1",
    "groq-sdk": "^0.3.2",
    "http-status-codes": "^2.3.0",
    "ignore": "^5.3.1",
    "jsdom": "^24.0.0",
    "lodash": "^4.17.20",
    "module-alias": "^2.2.2",
    "openai": "^4.28.4",
    "pino": "^8.18.0",
    "pino-pretty": "^11.0.0",
    "playwright": "^1.42.1",
    "puppeteer": "^22.6.5",
    "readline": "^1.3.0",
    "serpapi": "^2.1.0",
    "ts-morph": "^21.0.1",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.9.0",
    "tslib": "^2.6.2",
    "turndown": "^7.1.3",
    "xmldom": "^0.6.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@swc/cli": "^0.3.9",
    "@swc/core": "^1.3.107",
    "@swc/helpers": "^0.5.3",
    "@types/chai-subset": "^1.3.3",
    "@types/debug": "^4.1.5",
    "@types/dotenv": "^6.1.1",
    "@types/lodash": "^4.14.149",
    "@types/mocha": "^7.0.2",
    "@types/node": "^20.0.0",
    "@types/pino": "^6.3.12",
    "@types/sinon": "^10.0.20",
    "@types/xmldom": "^0.1.34",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "chai": "^4.4.1",
    "chai-subset": "^1.6.0",
    "drizzle-kit": "^0.20.14",
    "mocha": "^10.3.0",
    "npm-run-all": "^4.1.5",
    "sinon": "^17.0.0",
    "ts-node-dev": "^2.0.0",
    "tsc-watch": "^4.0.0",
    "typescript": "^5.3.3"
  }
}
